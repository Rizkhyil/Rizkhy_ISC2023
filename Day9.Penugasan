package Day9.Increment;

public class Main {
    
    public static void main(String[] args) {
        /*
        Post increment
        a++
        pre increment
        ++a
        */
        
        int a;
        int y = 6;
        a = y++ ;
        
        System.out.println(a);
        System.out.println(y);
        
        
        
    }
    
}
